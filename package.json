{
  "name": "isthepatchout-notifications",
  "type": "module",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@9.7.0",
  "engines": {
    "node": "^21"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,html,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "build": "tsup",
    "dev": "onchange --initial --kill .env 'src/**' pnpm-lock.yaml -- pnpm --silent dev:start",
    "dev:start": "pnpm --silent build; pnpm --silent start",
    "lint": "eslint src",
    "prepare": "is-ci || simple-git-hooks",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit --project tsconfig.json"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.25.1",
    "@supabase/realtime-js": "2.9.3",
    "@supabase/supabase-js": "2.40.0",
    "@tsconfig/node21": "21.0.2",
    "@tsconfig/strictest": "2.0.4",
    "@types/node": "20.11.30",
    "@types/node-fetch": "2.6.11",
    "@types/pg": "8.11.4",
    "@types/web-push": "3.6.3",
    "dotenv": "16.4.5",
    "drizzle-orm": "0.30.4",
    "eslint": "9.9.0",
    "fetch-unfiller": "0.2.0",
    "is-ci": "3.0.1",
    "lint-staged": "15.2.2",
    "ofetch": "1.3.4",
    "onchange": "7.1.0",
    "p-queue": "8.0.1",
    "pg": "8.11.3",
    "pino": "8.19.0",
    "pino-pretty": "11.0.0",
    "postgres": "3.4.4",
    "prettier": "3.2.5",
    "remeda": "1.56.0",
    "simple-git-hooks": "2.11.1",
    "tsup": "8.2.4",
    "typescript": "5.5.4",
    "web-push": "3.6.7"
  },
  "pnpm": {
    "overrides": {
      "is-core-module": "npm:@nolyfill/is-core-module@^1"
    }
  }
}
