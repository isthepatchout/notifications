{
  "name": "isthepatchout-notifications",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.1.1",
  "engines": {
    "node": "^18"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,html,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "lint": "eslint src",
    "prepare": "simple-git-hooks",
    "start": "tsx src/index",
    "typecheck": "tsc --noEmit --project tsconfig.json"
  },
  "resolutions": {
    "@types/node": "18.7.14"
  },
  "dependencies": {
    "@sentry/node": "7.11.1",
    "@supabase/supabase-js": "2.0.0-rc.8",
    "axios": "0.27.2",
    "dotenv": "16.0.2",
    "p-queue": "6.6.2",
    "pino": "7.11.0",
    "pino-pretty": "7.6.1",
    "web-push": "3.5.0"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.7.0",
    "@tsconfig/node16": "1.0.3",
    "@types/node": "18.7.14",
    "@types/web-push": "3.3.2",
    "eslint": "8.22.0",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "simple-git-hooks": "2.8.0",
    "tsx": "3.9.0",
    "typescript": "4.8.2"
  }
}
