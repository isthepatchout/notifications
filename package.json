{
  "name": "isthepatchout-notifications",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,html,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "ts-node-dev src/index",
    "lint": "eslint src",
    "postinstall": "simple-git-hooks",
    "start": "ts-node src/index",
    "typecheck": "tsc --noEmit --project tsconfig.json"
  },
  "dependencies": {
    "@sentry/node": "6.7.1",
    "@supabase/supabase-js": "1.15.1",
    "bottleneck": "2.19.5",
    "pino": "6.11.3",
    "pino-pretty": "5.0.2",
    "web-push": "3.4.4"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.2.1",
    "@tsconfig/node16": "1.0.1",
    "@types/pino": "6.3.8",
    "@types/web-push": "3.3.0",
    "dotenv": "10.0.0",
    "eslint": "7.28.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.1",
    "simple-git-hooks": "2.4.1",
    "ts-node": "10.0.0",
    "ts-node-dev": "1.1.6",
    "typescript": "4.3.3"
  }
}
