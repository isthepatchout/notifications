{
  "name": "isthepatchout-notifications",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.1.1",
  "engines": {
    "node": ">=16"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,html,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "ts-node-dev src/index",
    "lint": "eslint src",
    "postinstall": "simple-git-hooks",
    "start": "ts-node src/index",
    "typecheck": "tsc --noEmit --project tsconfig.json"
  },
  "resolutions": {
    "@types/node": "16.4.12"
  },
  "dependencies": {
    "@sentry/node": "6.11.0",
    "@supabase/supabase-js": "1.22.3",
    "bottleneck": "2.19.5",
    "pino": "6.13.1",
    "pino-pretty": "6.0.0",
    "web-push": "3.4.5"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.2.1",
    "@tsconfig/node16": "1.0.2",
    "@types/node": "16.7.5",
    "@types/pino": "6.3.11",
    "@types/web-push": "3.3.2",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2",
    "simple-git-hooks": "2.6.1",
    "ts-node": "10.2.1",
    "ts-node-dev": "1.1.8",
    "typescript": "4.4.2"
  }
}
