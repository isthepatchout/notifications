{
  "name": "isthepatchout-notifications",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.1.1",
  "engines": {
    "node": "^18"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,html,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "lint": "eslint src",
    "prepare": "simple-git-hooks",
    "start": "tsx src/index",
    "typecheck": "tsc --noEmit --project tsconfig.json"
  },
  "resolutions": {
    "@types/node": "18.7.14"
  },
  "dependencies": {
    "@sentry/node": "7.19.0",
    "@supabase/supabase-js": "2.1.0",
    "dotenv": "16.0.3",
    "knex": "2.3.0",
    "mande": "2.0.3",
    "p-queue": "6.6.2",
    "pg": "8.8.0",
    "pino": "8.7.0",
    "pino-pretty": "9.1.1",
    "remeda": "1.3.0",
    "web-push": "3.5.0"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.7.0",
    "@tsconfig/node18": "1.0.1",
    "@types/node": "18.11.9",
    "@types/pg": "8.6.5",
    "@types/web-push": "3.3.2",
    "eslint": "8.22.0",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "simple-git-hooks": "2.8.1",
    "tsx": "3.12.1",
    "typescript": "4.8.4"
  }
}
