{
  "name": "isthepatchout-notifications",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.1.1",
  "engines": {
    "node": "^16.15"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,html,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "lint": "eslint src",
    "prepare": "simple-git-hooks",
    "start": "tsx src/index",
    "typecheck": "tsc --noEmit --project tsconfig.json"
  },
  "resolutions": {
    "@types/node": "16.11.49"
  },
  "dependencies": {
    "@sentry/node": "7.11.1",
    "@supabase/supabase-js": "2.0.0-rc.3",
    "axios": "0.26.0",
    "bottleneck": "2.19.5",
    "dotenv": "16.0.1",
    "pino": "7.8.0",
    "pino-logflare": "0.3.12",
    "pino-pretty": "7.5.1",
    "web-push": "3.4.5"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.7.0",
    "@tsconfig/node16": "1.0.3",
    "@types/node": "16.11.49",
    "@types/web-push": "3.3.2",
    "eslint": "8.22.0",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "simple-git-hooks": "2.8.0",
    "tsx": "3.8.2",
    "typescript": "4.7.4"
  }
}
