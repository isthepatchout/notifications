{
  "name": "isthepatchout-notifications",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@7.25.0",
  "engines": {
    "node": "^18"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,html,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "lint": "eslint src",
    "prepare": "simple-git-hooks",
    "start": "tsx src/index",
    "typecheck": "tsc --noEmit --project tsconfig.json"
  },
  "resolutions": {
    "@types/node": "18.7.14"
  },
  "dependencies": {
    "@sentry/node": "7.31.1",
    "@supabase/supabase-js": "2.4.0",
    "dotenv": "16.0.3",
    "knex": "2.4.1",
    "mande": "2.0.3",
    "p-queue": "6.6.2",
    "pg": "8.8.0",
    "pino": "8.8.0",
    "pino-pretty": "9.1.1",
    "remeda": "1.6.0",
    "web-push": "3.5.0"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "1.1.2",
    "@tsconfig/node18": "1.0.1",
    "@types/node": "18.11.18",
    "@types/pg": "8.6.6",
    "@types/web-push": "3.3.2",
    "eslint": "8.32.0",
    "lint-staged": "13.1.0",
    "prettier": "2.8.3",
    "simple-git-hooks": "2.8.1",
    "tsx": "3.12.2",
    "typescript": "4.9.4"
  }
}
