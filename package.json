{
  "name": "isthepatchout-notifications",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@7.27.0",
  "engines": {
    "node": "^18"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,html,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "onchange --initial --kill .env 'src/**' pnpm-lock.yaml -- pnpm --silent start",
    "lint": "eslint src",
    "prepare": "simple-git-hooks",
    "start": "tsx src/index",
    "typecheck": "tsc --noEmit --project tsconfig.json"
  },
  "resolutions": {
    "@types/node": "18.16.3"
  },
  "dependencies": {
    "@sentry/node": "7.54.0",
    "@supabase/realtime-js": "2.7.2",
    "@supabase/supabase-js": "2.24.0",
    "@tsconfig/node18": "2.0.1",
    "@tsconfig/strictest": "2.0.1",
    "dotenv": "16.1.4",
    "knex": "2.4.2",
    "ofetch": "1.0.1",
    "p-queue": "6.6.2",
    "pg": "8.11.0",
    "pino": "8.14.1",
    "pino-pretty": "10.0.0",
    "remeda": "1.19.0",
    "tsx": "3.12.7",
    "typescript": "5.1.3",
    "web-push": "3.6.1"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "2.1.1",
    "@types/node": "18.16.3",
    "@types/node-fetch": "2.6.4",
    "@types/pg": "8.10.1",
    "@types/web-push": "3.3.2",
    "eslint": "8.42.0",
    "lint-staged": "13.2.2",
    "onchange": "7.1.0",
    "prettier": "2.8.8",
    "simple-git-hooks": "2.8.1"
  },
  "pnpm": {
    "patchedDependencies": {
      "ofetch@1.0.1": "patches/ofetch@1.0.1.patch"
    }
  }
}
