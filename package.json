{
  "name": "isthepatchout-notifications",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@8.14.1",
  "engines": {
    "node": "^20"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,html,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "onchange --initial --kill .env 'src/**' pnpm-lock.yaml -- pnpm --silent start",
    "lint": "eslint src",
    "prepare": "simple-git-hooks",
    "start": "tsx src/index",
    "typecheck": "tsc --noEmit --project tsconfig.json"
  },
  "dependencies": {
    "@sentry/node": "7.56.0",
    "@supabase/realtime-js": "2.7.3",
    "@supabase/supabase-js": "2.26.0",
    "@tsconfig/node20": "20.1.2",
    "@tsconfig/strictest": "2.0.2",
    "dotenv": "16.3.2",
    "knex": "2.4.2",
    "ofetch": "1.1.1",
    "p-queue": "6.6.2",
    "pg": "8.11.1",
    "pino": "8.14.1",
    "pino-pretty": "10.0.0",
    "remeda": "1.23.0",
    "tsx": "4.7.0",
    "typescript": "5.3.3",
    "web-push": "3.6.3"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "4.0.0",
    "@types/node": "20.11.5",
    "@types/node-fetch": "2.6.11",
    "@types/pg": "8.10.9",
    "@types/web-push": "3.6.3",
    "eslint": "8.56.0",
    "lint-staged": "15.2.0",
    "onchange": "7.1.0",
    "prettier": "3.2.4",
    "simple-git-hooks": "2.9.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "ofetch@1.1.1": "patches/ofetch@1.1.1.patch"
    }
  }
}
